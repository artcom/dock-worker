    "log-update": "^1.0.2",
    "read": "^1.0.7",
    "text-table": "^0.2.0",
    "yn": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.4",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.8.0",
    "mocha": "^2.3.3",
    "sinon": "^1.17.2",
    "tmp": "0.0.28"
  },
  "license": "MIT"
