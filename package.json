{
  "name": "dock-worker",
  "version": "0.5.0",
  "description": "Automated Dokku Deployment",
  "repository": "artcom/dock-worker",
  "files": [
    "dist/"
  ],
  "bin": {
    "dock": "dist/main.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "debug": "npm run prepublishOnly && node --inspect-brk dist/main.js",
    "start": "npm run prepublishOnly && node dist/main.js",
    "test": "npm run type-check && mocha -r ts-node/register test/**/*.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "dependencies": {
    "bl": "^2.1.2",
    "chalk": "^2.4.1",
    "docopt": "^0.6.2",
    "elegant-spinner": "^1.0.1",
    "lodash": "^4.0.1",
    "log-update": "^2.3.0",
    "read-promise": "^1.0.1",
    "ssh2": "^0.6.1",
    "strip-ansi": "^5.0.0",
    "text-table": "^0.2.0",
    "yn": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@types/bl": "^2.1.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/docopt": "^0.6.31",
    "@types/elegant-spinner": "^1.0.0",
    "@types/lodash": "^4.14.118",
    "@types/log-update": "^2.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.2",
    "@types/sinon": "^5.0.5",
    "@types/ssh2": "^0.5.36",
    "@types/strip-ansi": "^3.0.0",
    "@types/text-table": "^0.2.0",
    "@types/tmp": "0.0.33",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.8.0",
    "eslint-config-artcom": "^0.9.1",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "tmp": "0.0.33",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.5",
    "typescript-eslint-parser": "^20.1.1"
  },
  "license": "MIT"
}
