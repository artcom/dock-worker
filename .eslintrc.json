{
  "extends": "artcom",
  "parser": "typescript-eslint-parser",
  "plugins": ["import", "typescript"],
  "rules": {
    "typescript/no-unused-vars": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-use-before-define": 0
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
